{
  "name": "jarvis-auth-system",
  "version": "1.0.0",
  "description": "J.A.R.V.I.S Authentication System with Google Sheets Integration",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:legacy": "set NODE_OPTIONS=--openssl-legacy-provider && next dev",
    "build": "next build",
    "build:legacy": "set NODE_OPTIONS=--openssl-legacy-provider && next build",
    "start": "next start",
    "start:legacy": "set NODE_OPTIONS=--openssl-legacy-provider && next start",
    "lint": "next lint",
    "test": "echo \"No tests specified\" && exit 0",
    "check-env": "node scripts/check-env.js",
    "prebuild": "npm run check-env",
    "security-check": "node scripts/security-check.js",
    "prepare-cleanup": "node scripts/prepare-cleanup.js",
    "clean-history": "node scripts/clean-history.js",
    "clean-history-windows": "powershell -ExecutionPolicy Bypass -File scripts\\clean-history-windows.ps1",
    "test-cleanup": "node scripts/test-cleanup.js",
    "deploy": "bash scripts/deploy.sh",
    "deploy:prod": "bash scripts/deploy.sh production",
    "backup": "bash scripts/backup.sh",
    "monitor": "bash scripts/monitor.sh",
    "docker:build": "docker build -t jarvis-auth-system .",
    "docker:run": "docker run -p 3000:3000 --env-file .env jarvis-auth-system",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "vercel": "vercel",
    "vercel:prod": "vercel --prod",
    "debug": "node scripts/debug.js",
    "debug:build": "node scripts/debug.js --build",
    "fix": "chmod +x fix.sh && ./fix.sh"
  },
  "dependencies": {
